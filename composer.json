{
  "name": "northrook/asset-manager",
  "description": "An asset manager written in PHP. Manage optimised, public versions of assets from various sources.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Northrook\\Asset\\": ""
    }
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "northrook/core": "dev-main",
    "northrook/html-element": "dev-main",
    "northrook/persistent-storage": "dev-main",
    "northrook/minify": "dev-main",
    "symfony/cache": "^7.1"
  },
  "require-dev": {
    "northrook/dev-env": "dev-main",
    "latte/latte": "^3.0"
  }
}