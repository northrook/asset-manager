{
  "name": "northrook/asset-manager",
  "description": "An asset manager written in PHP. Manage optimised, public versions of assets from various sources.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Northrook\\Asset\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "symfony/filesystem": "^7.1",
    "symfony/finder": "^7.1",
    "symfony/cache": "^7.0",
    "northrook/core": "dev-main",
    "northrook/logger": "dev-main",
    "northrook/support": "dev-main",
    "northrook/elements": "dev-main"
  },
  "require-dev": {
    "symfony/var-dumper": "^7.0",
    "northrook/symfony-core-bundle": "dev-main"
  }
}